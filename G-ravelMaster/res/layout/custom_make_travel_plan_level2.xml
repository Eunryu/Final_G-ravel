<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/custom_iv_MakeTravelPlanLevel2_InfoImage"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:src="@drawable/ic_launcher" />

    <TextView
        android:id="@+id/custom_tv_MakeTravelPlanLevel2_ShopName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/All_None" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/custom_tv_MakeTravelPlanLevel2_ShopArea"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:text="@string/All_None" />

        <TextView
            android:id="@+id/custom_tv_MakeTravelPlanLevel2_ShopSelect"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/All_None" />
    </LinearLayout>

</LinearLayout>